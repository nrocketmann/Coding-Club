{
  "manifest_version": 2,
  "name": "Lick stuff",
  "description": "This extension helps consolidate all of your lick information in an easier to use form",
  "version": "1.0",
  "background": {
    "css": ["app-ui/build/static/css/main.08fe32c0.css", "static/css/main.08fe32c0.css.map"],
    "js": ["app-ui/build/static/js/main.7ea871be.js", "static/js/main.7ea871be.js.map"]

  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "app-ui/build/index.html",
    "default_title": "LWHS app"
  },
  "permissions": [
    "*://*.myschoolapp.com/*",
    "cookies",
    "https://ajax.googleapis.com/",
    "*://*.myschoolapp.com/"
  ]

}
